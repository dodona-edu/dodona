# Ignore secret keys
config/keys.yml

# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/yarn-error.log

/public/packs
/public/packs-test/
/node_modules

# Ignore Byebug command history file.
.byebug_history

# Ignore exercises
/data
!/data/.keep

# Ignore media files
/public/exercises
/.project

# Ignore generated entity-relationship diagram (rails-erd gem)
erd.pdf

# Ignore coverage reports
coverage
/public/packs
/public/packs-test
/node_modules
/public/packs
/public/packs-test
/node_modules
yarn-debug.log*
.yarn-integrity

# Ignore test reports
test/reports/
junit.xml

# Ignore master key for decrypting credentials and more.
/config/master.key

/config/credentials/staging.key
