<% user.api_tokens.each do |token| %>
  <tr id="api-token-<%= token.id %>">
    <td><%= token.description %></td>
    <td><%= l token.created_at %></td>
    <td class="text-right">
      <%= button_to api_token_path(token), method: :delete, remote: true, class: 'btn btn-sm btn-danger' do %>
        <span class="glyphicon glyphicon-remove"></span>
      <% end %>
    </td>
  </tr>
<% end %>
