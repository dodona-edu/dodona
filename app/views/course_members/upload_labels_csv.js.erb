<% if @error.present? %>
$("<div style=\"display:none\" class=\"alert alert-danger alert-dismissible\"> <button type=\"button\" class=\"close\" data-dismiss=\"alert\"><span>&times;</span></button><%= @error %></div>").insertBefore("#labelsUploadModal .modal-body").show("fast");
<% else %>
%("#labelsUploadModal").modal("hide");
<% end %>
