<% if @repository %>
dodona.setHTMLExecuteScripts(document.querySelector("#courses-table-wrapper"), "<%= escape_javascript(render partial: 'courses/repo_courses_table', locals: {courses: @courses}) %>")
if (dodona.repositoryCoursesLoaded) {
    dodona.repositoryCoursesLoaded();
}
<% elsif @copy_courses %>
dodona.setHTMLExecuteScripts(document.querySelector("#courses-table-wrapper"), "<%= escape_javascript(render partial: 'courses/copy_courses_table', locals: {courses: @courses}) %>")
if (dodona.copyCoursesLoaded) {
    dodona.copyCoursesLoaded();
}
<% else %>
dodona.setHTMLExecuteScripts(document.querySelector("#courses-table-wrapper"), "<%= escape_javascript(render partial: 'courses/courses_table', locals: {courses: @courses}) %>")
<% end %>
