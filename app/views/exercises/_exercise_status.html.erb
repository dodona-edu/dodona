<% if exercise.access_private? && local_assigns[:series].nil? %>
  <i class="material-icons material-icons-strikethrough" title="<%= t('exercises.index.private') %>">remove_red_eye</i>
<% if exercise.removed? %>
  <i class="material-icons" title="<%= t('exercises.index.removed') %>">delete</i>
<% elsif exercise.not_valid? %>
  <i class="material-icons" title="<%= t('exercises.index.invalid') %>">error</i>
<% end %>
