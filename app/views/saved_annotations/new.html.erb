<div class="row">
  <div class="col-12">
    <div class="card">
      <div class="card-title card-title-colored">
        <h2 class="card-title-text"><%= t ".title" %></h2>
      </div>
      <div class="card-supporting-text" id="annotation-container">
        <%= render partial: 'layouts/searchbar', locals: { refresh_element: "#annotation-container", courses: @courses, exercises: @exercises } %>
        <div id="questions-table-wrapper">
          <%= render partial: 'annotations/annotations_table', locals: { annotations: @annotations, allow_save: true } %>
        </div>
      </div>
    </div>
  </div>
</div>
