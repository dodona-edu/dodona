$("#submissions-table-wrapper").html("<%= escape_javascript(render partial: 'submissions_table', locals: {submissions: @submissions, exercise: @exercise, course: @course, remote_links: false}) %>");
if (dodona.feedbackTableLoaded) {
    dodona.feedbackTableLoaded(
        <%= raw "#{current_user&.id || 'undefined'}" %>,
        <%= raw "#{@exercise&.id || 'undefined'}" %>,
        <%= raw "#{@course&.id || 'undefined'}" %>
    );
}
